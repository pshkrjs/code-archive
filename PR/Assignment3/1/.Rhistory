if (is.null(colnames(x)))
colnames(x) = paste(rep("X", ncol(x)), seq_len(ncol(x)),
sep = "")
if (is.null(rownames(x)))
rownames(x) = 1L:nrow(x)
list(X = x, y = y)
}
